{
  "expo": {
    "name": "mekanik",
    "slug": "mekanik",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mekanik",
    "icon": "./assets/images/newlogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/newlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/images/notificationlogo.png"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.geepytech.mekanikapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/newlogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.geepytech.mekanikapp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/images/newlogo.png"
    },
    "extra": {
      "eas": {
        "projectId": "820c2510-a2a9-4ac2-ad83-2446baa88f81"
      }
    }
  }
}
